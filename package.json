{
  "name": "company-identifier-validator",
  "version": "0.0.0-development",
  "description": "Validate company identifier for many countries",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "author": "Michael Desigaud",
  "bugs": {
    "url": "https://github.com/RedFroggy/company-identifier-validator/issues"
  },
  "keywords": [
    "business",
    "company",
    "identifier",
    "international",
    "validator",
    "red froggy"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RedFroggy/company-identifier-validator.git"
  },
  "scripts": {
    "build": "npm run build:main && npm run build:module",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "codecov": "codecov",
    "test": "jest --config ./test/jest.config.js",
    "test:ci": "jest --config ./test/jest.config.js --coverage",
    "lint": "eslint src --ext .ts",
    "watch:build": "tsc -p tsconfig.json -w",
    "doc": "npm run doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "countries-db": "1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jasmine": "3.6.3",
    "@types/jasminewd2": "2.0.8",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "codecov": "3.8.1",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-functional": "3.2.1",
    "eslint-plugin-import": "2.22.1",
    "gh-pages": "3.1.0",
    "husky": "4.3.8",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "jest": "26.6.3",
    "jest-html-reporter": "3.3.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "semantic-release": "17.3.7",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typedoc": "0.20.19",
    "typescript": "^4.0.2"
  },
  "prettier": {
    "singleQuote": true
  }
}
